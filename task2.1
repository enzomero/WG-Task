# -*- coding: utf-8 -*-
import time


# describtion
def interF(value):
    temp_value = 1
    for x in range(1, value+1):
        temp_value*=x
    return temp_value;

# describtion
def recurF(value):
    if value < 1:
        return 1
    else:
        return value * recurF(value - 1)

def checkTime(val):
    t1=time.time()
    recurF(val)
    t2=time.time()
    r = t2-t1;
    return r;

print(checkTime(20))


