# -*- coding: utf-8 -*-
import time

# describtion
def interF(value):
    temp_value = 1
    for x in range(1, value+1):
        temp_value*=x
    return temp_value;

# describtion
def recurF(v):
    if v < 1:
        return 1
    else:
        return v * recurF(v - 1)
    

def checkTimeR(val):
    print('Recurcive',time.time(), sep='\n')
    try:
        recurF(val)
    except RuntimeError as err:
        print(err)
    print(time.time(), '\n')
    return;

def checkTimeI(val):
    print('Interaction',time.time(), sep='\n')
    try:
        interF(val)
    except RuntimeError as err:
        print(err)
    print(time.time(), '\n')
    return;

def run():
    nb = input('Enter the numb:\n')
    q = int(nb) # Be carefull, it is like int_32
    checkTimeR(q)
    checkTimeI(q)
    return;

run()
